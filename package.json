    "angular-animate": "^1.6.4",
    "angular-aria": "^1.6.4",
    "angular-material": "^1.1.3",
    "angular-messages": "^1.6.4",
    "angular-mocks": "^1.5.0",
    "angular-ui-router": "^0.4.2",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "restangular": "^1.6.1",
    "roboto-fontface": "^0.7.0"
